    "vue-router": "^3.1.5"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/webpack-chain": "^4.8.1",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "express": "^4.16.4",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "lint-staged": "^8.1.5",
